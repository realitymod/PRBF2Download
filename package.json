{
  "name": "PRBF2-Download-Assistant",
  "version": "1.0.0",
  "description": "An automatic download tool for Project Reality: Battlefield 2",
  "main": "./app/main.js",
  "dependencies": {
    "humanize-duration": "latest",
    "pretty-bytes": "latest",
    "webtorrent": "latest"
  },
  "scripts": {
    "start": "electron .",
    "dist": "build"
  },
  "author": "Wouter Jansen",
  "build": {
    "appId": "PRBF2-Download-Assistant",
    "win": {
      "target": [
        "portable"
      ],
      "icon": "/app/assets/icons/ico/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  }
}
